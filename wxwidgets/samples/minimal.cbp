<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="minimal" />
		<Option execution_dir="." />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="wx3.1GTK2 DLL">
				<Option output="bin/$(APP_NAME)/wxdll/$(APP_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/wxdll" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-D__WXGTK__" />
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/$(WX_TOOLKIT)-unicode-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="wx_msw$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
					<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
				</Linker>
				<Environment>
					<Variable name="APP_NAME" value="minimal" />
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_TOOLKIT" value="gtk2" />
					<Variable name="WX_VERSION" value="3.1" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="wx3.1GTK2 DLL;" />
		</VirtualTargets>
		<ResourceCompiler>
			<Add directory="%WXWIN%/include" />
		</ResourceCompiler>
		<Unit filename="minimal/minimal.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
